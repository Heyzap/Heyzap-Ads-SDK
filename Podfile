source 'https://github.com/Heyzap/CocoapodsSource.git'
source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!

platform :ios, '6.0'

target 'SampleApp' do
    pod 'Heyzap', '9.1.5' # Maintained by Heyzap
    
    pod 'ChartboostSDK', '5.5.3', :inhibit_warnings => true # Maintained by Chartboost
    pod 'AdColony', '2.5.3' # Maintained by AdColony
    pod 'Google-Mobile-Ads-SDK', '7.4.1' # Maintained by Google
    pod 'VungleAdvertiserSDK', '3.1.2' # In Cocoapods, but outdated. Vendored by Heyzap
    pod 'AppLovin', '3.1.2' # Vendored by Heyzap
    pod 'UnityAds', '1.4.5' # Vendored by Heyzap
    pod 'FacebookAudienceNetwork', '4.5.1' # Vendored by Heyzap
    pod 'HyprMX', '36' # Vendored by Heyzap
    pod 'Leadbolt', '5.2' # Vendored by Heyzap
end
