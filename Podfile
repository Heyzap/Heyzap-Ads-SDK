source 'https://github.com/Heyzap/CocoapodsSource.git'
source 'https://github.com/CocoaPods/Specs.git'

inhibit_all_warnings!

platform :ios, '7.0'

target 'SampleApp' do
    pod 'Heyzap', '9.2.5' # Maintained by Heyzap
    
    pod 'ChartboostSDK', '5.5.3', :inhibit_warnings => true # Maintained by Chartboost
    pod 'AdColony', '2.5.3' # Maintained by AdColony
    pod 'Google-Mobile-Ads-SDK', '7.5.0' # Maintained by Google
    pod 'VungleAdvertiserSDK', '3.2.0' # In Cocoapods, but outdated. Vendored by Heyzap
    pod 'AppLovin', '3.1.5' # Vendored by Heyzap
    pod 'UnityAds', '1.5.2' # Vendored by Heyzap
    pod 'FacebookAudienceNetwork', '4.5.1' # Vendored by Heyzap
    pod 'HyprMX', '68' # Vendored by Heyzap
    pod 'Leadbolt', '6.0' # Vendored by Heyzap
end
